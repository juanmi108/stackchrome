{
    "name": "Stackoverflow Ask",
    "version": "0.0.2",
    "description": "__MSG_extDescription__",
    "permissions": [
        "tabs",
        "scripting"
    ],
    "host_permissions": [
        "http://*/",
        "https://*/"
    ],
    "action": {
        "default_popup": "popups/default.html",
        "default_title": "__MSG_action_icon_title__"
    },
    "icons": {
        "64": "icons/stack-overflow-64.png"
    },
    "manifest_version": 3,
    "minimum_chrome_version": "92",
    "default_locale": "en"
}
